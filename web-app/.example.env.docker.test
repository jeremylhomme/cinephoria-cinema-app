#These are the environment variables that are used in Docker to run the tests.

PORT=5000

# MongoDB Config
MONGODB_HOST=mongoContainerName
MONGODB_USERNAME=user
MONGODB_PASSWORD=password
MONGO_INITDB_ROOT_USERNAME=root
MONGO_INITDB_ROOT_PASSWORD=exampleRootPassword
MONGODB_NAME=exampleDB
MONGODB_PORT=27017
MONGODB_URI=mongodb://root:exampleRootPassword@localhost:27017/exampleDB?authSource=admin

# MySQL Config
MYSQL_HOST=mysqlContainerName
MYSQL_PASSWORD=examplePassword
MYSQL_ROOT_PASSWORD=exampleRootPassword
MYSQL_DB_NAME=exampleDB
MYSQL_PORT=3306  

MAILGUN_CINEPHORIA_API_KEY=exampleMailgunApiKey
MAILGUN_DOMAIN=example.mailgun.org

# Prisma Config
DATABASE_URL=mysql://user:password@mysqlContainerName:3306/exampleDB

# JWT Secret
JWT_SECRET=exampleJwtSecretKey